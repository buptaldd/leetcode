class Solution {
    public int maxArea(int[] height) {
        int i = 0 , j = height.length ;
        int maxa=(j-i)*Math.min(height[i],height[j]) ;
        for (;i<j;){
            if (height[i]<height[j]){
                i++ ;
            }else{
                j++ ;
            }
            int temp = (j-i)*Math.min(height[i],height[j]) ;
            maxa = Math.max(maxa,temp);
        }
        return maxa;        
    }
}
